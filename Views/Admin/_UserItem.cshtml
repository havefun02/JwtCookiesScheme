@model JwtCookiesScheme.ViewModels.UserItemViewModel
<tr>
    <td>@Model.UserName</td>
    <td>@Model.UserEmail</td>
    <td>@Model.FullName()</td>
    <td>@Model.UserPhone</td>
    <td>@Model.RoleString()</td>

    <td>
        <a asp-action="Edit" asp-controller="Admin" asp-route-id="@Model.UserId" >Edit</a> |
        <a asp-action="Delete" asp-controller="Admin" asp-route-id="@Model.UserId">Delete</a>
    </td>
</tr>